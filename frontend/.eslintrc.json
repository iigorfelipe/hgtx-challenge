{
    "env": {
        "es2021": true,
        "browser": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb-base",
        "airbnb-typescript/base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "project": "./frontend/tsconfig.json"
    },
    "plugins": [
        "react",
        "@typescript-eslint"
    ],
    "rules": {
        "no-console": "off"
    }
}
